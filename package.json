{
  "name": "crypto-binance-listener-optimized",
  "version": "2.0.0",
  "description": "Optimized Node.js scripts to listen to Binance and relay data with low latency.",
  "main": "binance_listener.js",
  "scripts": {
    "start-listener": "node binance_listener.js",
    "start-receiver": "node data_receiver_server.js",
    "pm2-start-listener": "pm2 start binance_listener.js --name binance-listener-prod",
    "pm2-start-receiver": "pm2 start data_receiver_server.js --name data-receiver-prod",
    "pm2-list": "pm2 list",
    "pm2-stop-all": "pm2 stop all && pm2 delete all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "crypto",
    "binance",
    "websocket",
    "nodejs",
    "uWebSockets",
    "low-latency",
    "data-feed"
  ],
  "author": "0zero4u",
  "license": "ISC",
  "dependencies": {
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.4.0"
  }
}
